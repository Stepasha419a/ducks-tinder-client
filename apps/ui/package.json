{
  "name": "@ducks-tinder-client/ui",
  "version": "1.0.1",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "storybook": "storybook dev -p 6006",
    "storybook:test": "test-storybook",
    "build": "rimraf ./dist && vite build",
    "build:dev:publish": "pnpm build && pnpm publish:dev",
    "build:dev:publish:override": "pnpm build && pnpm publish:dev:override",
    "up:dev:packages": "pnpm cache delete @ducks-tinder-client/* && pnpm up @ducks-tinder-client/*",
    "up:dev:patch": "pnpm version patch && pnpm up:dev:packages && pnpm build:dev:publish:override",
    "up:dev:minor": "pnpm version minor && pnpm up:dev:packages && pnpm build:dev:publish:override",
    "lint": "eslint \"./src/**/*{.ts,.tsx,.js,.jsx}\" --max-warnings=0",
    "lint:fix": "eslint \"./src/**/*{.ts,.tsx,.js,.jsx}\" --fix --max-warnings=0",
    "publish:dev": "npm publish --registry http://localhost:4873/",
    "publish:dev:override": "npm unpublish --force --registry http://localhost:4873/ && pnpm publish:dev"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "@ducks-tinder-client/common": "1.1.x",
    "@fortawesome/fontawesome-common-types": "6.4.x",
    "@fortawesome/fontawesome-svg-core": "6.4.x",
    "@fortawesome/free-solid-svg-icons": "6.4.x",
    "@fortawesome/react-fontawesome": "0.1.x",
    "axios": "0.28.x",
    "browserslist": "4.18.x",
    "classnames": "2.3.x",
    "framer-motion": "10.12.x",
    "react": "18.2.x",
    "react-dom": "18.2.x",
    "react-toastify": "9.1.x"
  },
  "dependencies": {
    "react-infinite-scroller": "^1.2.6",
    "react-loading-skeleton": "^3.3.1",
    "react-slick": "^0.29.0",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@feature-sliced/eslint-config": "^0.1.1",
    "@storybook/addon-essentials": "^8.5.0",
    "@storybook/addon-interactions": "^8.5.0",
    "@storybook/addon-links": "8.5.0",
    "@storybook/addon-onboarding": "^8.5.0",
    "@storybook/blocks": "^8.5.0",
    "@storybook/builder-vite": "^8.5.0",
    "@storybook/react": "^8.5.0",
    "@storybook/react-vite": "^8.5.0",
    "@storybook/test": "^8.5.0",
    "@storybook/test-runner": "^0.21.0",
    "@storybook/testing-library": "0.2.2",
    "@types/node": "^22.13.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.21",
    "@types/react-slick": "^0.23.13",
    "@types/toastify-js": "^1.11.1",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8.37.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-boundaries": "^5.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.11.2",
    "prettier": "^2.8.8",
    "rimraf": "^6.0.1",
    "sass": "^1.83.4",
    "storybook": "^8.5.0",
    "typescript": "^5.0.3",
    "vite": "^6.0.7",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-dts": "^4.5.0"
  }
}
