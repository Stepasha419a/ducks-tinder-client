{
  "name": "stepasha419a-ducks-tinder-client",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host --mode dev",
    "demo": "vite --host --mode demo",
    "build": "vite build",
    "preview": "vite preview",
    "prettier": "prettier --check src/**/*{.css,.scss,.ts,.tsx,.js,.jsx}",
    "prettier:fix": "prettier --write src/**/*{.css,.scss,.ts,.tsx,.js,.jsx}",
    "lint": "eslint ./src/**/*{.ts,.tsx,.js,.jsx}",
    "lint:fix": "eslint ./src/**/*{.ts,.tsx,.js,.jsx} --fix",
    "storybook": "storybook dev -p 6006",
    "storybook:test": "test-storybook"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-common-types": "^6.4.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.1.19",
    "@pbe/react-yandex-maps": "^1.2.4",
    "@reduxjs/toolkit": "^1.8.1",
    "@vercel/analytics": "^1.3.1",
    "@vercel/speed-insights": "^1.0.12",
    "axios": "^0.27.2",
    "browserslist": "^4.18.1",
    "classnames": "^2.3.2",
    "events": "^3.3.0",
    "framer-motion": "^10.12.18",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-easy-crop": "^4.5.0",
    "react-hook-form": "^7.43.4",
    "react-infinite-scroller": "^1.2.6",
    "react-loading-skeleton": "^3.3.1",
    "react-redux": "^8.0.1",
    "react-refresh": "^0.14.0",
    "react-router-dom": "^6.14.1",
    "react-slick": "^0.29.0",
    "react-toastify": "^9.1.3",
    "redux-thunk": "^2.4.1",
    "slick-carousel": "^1.8.1",
    "socket.io-client": "^4.5.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@feature-sliced/eslint-config": "^0.1.1",
    "@storybook/addon-essentials": "^7.1.1",
    "@storybook/addon-interactions": "^7.2.1",
    "@storybook/addon-links": "^7.1.1",
    "@storybook/blocks": "^7.1.1",
    "@storybook/builder-vite": "^8.0.8",
    "@storybook/jest": "^0.1.0",
    "@storybook/react": "^7.6.17",
    "@storybook/react-vite": "^8.0.8",
    "@storybook/test-runner": "^0.12.0",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "@types/react-infinite-scroller": "^1.2.5",
    "@types/react-slick": "^0.23.10",
    "@types/toastify-js": "^1.11.1",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vitejs/plugin-react": "^4.2.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.37.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "sass": "^1.58.3",
    "storybook": "^7.1.1",
    "typescript": "^5.0.3",
    "vite": "^5.2.7",
    "vite-plugin-checker": "^0.6.4"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  }
}
